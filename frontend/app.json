{
  "expo": {
    "name": "Aashiqo",
    "slug": "aashiqo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.framecrest.aashiqo"
    },
    "android": {
      "package": "com.framecrest.aashiqo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2cf4f64a-f65e-4a78-9e05-d79677a03623"
      }
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
